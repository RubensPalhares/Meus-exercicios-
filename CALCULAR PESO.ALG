Algoritmo "Calcular peso"

Escreva ("Digite seu Peso:")
leia (peso)
Escreva ("Digite sua Altura:")
leia (Altura)
imc <- peso / (altura ^2)
Escreva ("IMC : " , IMC:5:2)

se (imc = 18.5) e (imc <= 24.9) entao

escreva ("Voce esta no peso NORMAL",imc)

se (imc = 29.9) entao
escreva ( "Voce esta com SOBREPESO", imc)
fimse
fimse
fimalgoritmo
